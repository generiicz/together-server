{
    "swagger": "2.0",
    "info": {
        "title": "Together Api Doc",
        "description": "Api Docs",
        "version": "0.1.0"
    },
    "basePath": "/app",
    "schemes": [
        "http"
    ],
    "consumes": [
        "application/json"
    ],
    "produces": [
        "application/json"
    ],
    "paths": {},
    "definitions": {
        "Id": {
            "properties": {
                "id": {
                    "type": "integer",
                    "format": "int32"
                }
            }
        },
        "UserId": {
            "properties": {
                "user_id": {
                    "type": "integer",
                    "format": "int32"
                }
            }
        },
        "Email": {
            "properties": {
                "email": {
                    "description": "must be a valid email address.",
                    "type": "string",
                    "format": "email",
                    "maxLength": 255,
                    "example": "example@example.com"
                }
            }
        },
        "Phone": {
            "properties": {
                "phone": {
                    "description": "[Validation Rule Details](https://regex101.com/r/iD1kP5/1), [Discussion](https://trello.com/c/n4Xqa2W5)",
                    "type": "string",
                    "pattern": "^\\d{1,4},\\d{5,10}$",
                    "example": "1,123456789"
                }
            }
        },
        "Avatar": {
            "properties": {
                "avatar": {
                    "type": "string"
                }
            }
        },
        "Cover": {
            "properties": {
                "cover": {
                    "type": "string"
                }
            }
        },
        "Location": {
            "properties": {
                "lat": {
                    "description": "min: -90f, max: 90f",
                    "type": "number",
                    "format": "float",
                    "maximum": 90,
                    "minimum": -90,
                    "example": 45.51695
                },
                "lng": {
                    "description": "min: -180f, max: 180f",
                    "type": "number",
                    "format": "float",
                    "maximum": 180,
                    "minimum": -180,
                    "example": -73.55481
                }
            }
        }
    },
    "responses": {
        "Ok": {
            "description": "`OK`"
        },
        "Created": {
            "description": "`CREATED`"
        },
        "BadRequest": {
            "description": "`BAD_REQUEST`"
        },
        "Unauthorized": {
            "description": "`UNAUTHORIZED`"
        },
        "NotFound": {
            "description": "`NOT_FOUND`"
        },
        "ConflictEmailExists": {
            "description": "`EMAIL_EXISTS`"
        },
        "Invalidation": {
            "description": "`VALIDATION_ERROR`"
        },
        "UnexpectedError": {
            "description": "`UNEXPECTED_ERROR`"
        }
    },
    "securityDefinitions": {
        "basic": {
            "type": "basic"
        }
    }
